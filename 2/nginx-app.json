{
    "id": "nginx-app",
    "instances": 1,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "docker.touk.pl/nginx-app",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    },
    "cpus": 0.1,
    "healthChecks": [
        {
            "protocol": "HTTP",
            "portIndex": 0,
            "timeoutSeconds": 5,
            "maxConsecutiveFailures": 3
        }
    ]
}
